import java.util.Scanner;

public class numbertri {
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=1;i<=n;i++)
		{
			for(int j=i;j<=n;j++)
			{
				if(i==1)
				{
					System.out.print(j);
				}
				if(j==1)
				{
					System.out.print(i);
				}
				else
				{
					System.out.println(" ");
				}
			}
		}
	}

}
